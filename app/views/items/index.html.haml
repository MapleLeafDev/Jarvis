.span8{align: "center"}
  %h2.lead Store
  %ul.nav.nav-tabs
    %li.active
      %a{:href => "#"} Items
    %li
      = link_to 'Purchases', purchases_path
  -@items.sort_by{ |t| t.price }.each do |item|
    =render partial: 'item', locals: { item: item }
.span2{align: "center"}
  %p
  -if current_user.user_type >= 20
    = link_to " Item", new_item_path, class: "i icon-plus btn"
    %p
  .lead Credits
  %div.well.well-small#credits
    -if current_user.user_type < 10
      .lead
        =current_user.credits
    -else
      %table{width: "100%"}
        -children.each do |member|
          %tr
            %td=member.name
            %td=member.credits