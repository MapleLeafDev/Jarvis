.span8{align: "center"}
  .row
    .span5
      %h2.lead Store
    .span2.well.well-small{id: "credits"}
      .lead
        = current_user.credits
        credits
  .row
    %ul.nav.nav-tabs
      %li.active
        %a{:href => "#"} Items
      %li
        = link_to 'Purchases', purchases_path
  .row
    .span2
      = link_to " Item", new_item_path, class: "i icon-plus btn btn-success btn-small"
      %p
  .row
    .span7
      %p
        -@items.each do |item|
          =render partial: 'item', locals: { item: item }